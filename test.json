$(document).ready(function(){
var $_window = $(window);

function siteProdNavActive(width) {
let $_prod_detail_tab_targets = null;
let $_tab_target_a_tags = null;

if(!(width < 768)) {
$_prod_detail_tab_targets = $('#prod_detail .categorize ._prod_detail_tab_target');
$_tab_target_a_tags = $('#prod_detail .categorize ._prod_detail_tab_fixed ul li a');
} else {
$_prod_detail_tab_targets = $('#prod_detail .categorize-mobile ._prod_detail_tab_target');
$_tab_target_a_tags = $('#prod_detail .categorize-mobile ._prod_detail_tab_fixed ul li a');
}

if(!$_prod_detail_tab_targets && !$_tab_target_a_tags) return;

$_prod_detail_tab_targets.each(function(){
if($_window.scrollTop() >= $(this).offset().top - 1){
var name = $(this).attr('data-target');
$_tab_target_a_tags.removeClass('active');
$_tab_target_a_tags.filter('[href="#' + name + '"]').addClass('active');
}
});

// 아이디가 없는 경우에는 첫번째 탭을 활성화
}

$_window.on('scroll', function(){
siteProdNavActive($_window.width());
});
$_window.on('resize', function() {
siteProdNavActive($_window.width());
});
});

